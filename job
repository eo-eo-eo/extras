local vu242 = game:GetService("PathfindingService")
local function vu259(p243)
    local v244 = vu4.Character.Humanoid
    local vu245 = vu4.Character.HumanoidRootPart
    local vu246 = p243.Character.HumanoidRootPart.Position
    local vu247 = vu242:CreatePath()
    local function vu248()
        vu247 = vu242:CreatePath()
        vu247:ComputeAsync(vu245.Position, vu246)
        return vu247:GetWaypoints()
    end
    local vu249 = vu248()
    local v250, v251, v252 = pairs(vu249)
    local vu253 = vu246
    local vu254 = 3
    while true do
        local vu255, v256 = v250(v251, v252)
        if vu255 == nil then
            break
        end
        v252 = vu255
        v244:MoveTo(v256.Position)
        if v256.Action == Enum.PathWaypointAction.Jump then
            v244:ChangeState(Enum.HumanoidStateType.Jumping)
        end
        local vu257 = nil
        vu257 = v244.MoveToFinished:Connect(function(p258)
            if p258 then
                if vu254 < (newDestination - vu253).magnitude then
                    print("Target player moved, recalculating path...")
                    vu253 = newDestination
                    vu249 = vu248()
                    vu255 = 1
                end
                vu257:Disconnect()
            else
                warn("Movement interrupted!")
                vu257:Disconnect()
            end
        end)
    end
end
