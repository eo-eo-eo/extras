local function vu34(p23)
    local v24, v25, v26 = pairs(game.Players:GetChildren())
    while true do
        local v27
        v26, v27 = v24(v25, v26)
        if v26 == nil then
            break
        end
        local v28 = # p23
        if v27.Name:sub(1, v28) == p23 then
            return v27.Name, v27.DisplayName
        end
    end
    local v29, v30, v31 = pairs(game.Players:GetChildren())
    while true do
        local v32
        v31, v32 = v29(v30, v31)
        if v31 == nil then
            break
        end
        local v33 = # p23
        if v32.DisplayName:sub(1, v33) == p23 then
            return v32.Name, v32.DisplayName
        end
    end
    return nil, nil
end
local function vu48(p35)
    local v36, v37, v38 = pairs(game.Players:GetChildren())
    while true do
        local v39
        v38, v39 = v36(v37, v38)
        if v38 == nil then
            break
        end
        local v40 = # p35
        local v41 = v39.Name:sub(1, v40)
        if string.lower(v41) == p35 then
            return v39
        end
    end
    local v42, v43, v44 = pairs(game.Players:GetChildren())
    while true do
        local v45
        v44, v45 = v42(v43, v44)
        if v44 == nil then
            break
        end
        local v46 = # p35
        local v47 = v45.DisplayName:sub(1, v46)
        if string.lower(v47) == p35 then
            return v45
        end
    end
    return nil, nil
end
