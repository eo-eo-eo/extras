            vu69(readfile("chatbothub_ais.cb"))
        else
            writefile("chatbothub_ais.cb", "")
        end
    end
end
local function vu78()
    if writeFileAvailable() then
        writefile("chatbothub_ais.cb", encodeAIs())
    end
end
