function getKeys(p14)
    local v15, v16, v17 = pairs(p14)
    local v18 = {}
    while true do
        local v19
        v17, v19 = v15(v16, v17)
        if v17 == nil then
            break
        end
        table.insert(v18, v17)
    end
    return v18
end
