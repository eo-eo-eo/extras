            if v116 == p111 then
                table.remove(_G.CHATBOTHUB_BLACKLISTEDCONTENT, v115)
            end
        end
        vu107:Refresh(_G.CHATBOTHUB_BLACKLISTEDCONTENT, true)
        vu107:Set("")
    end
end
