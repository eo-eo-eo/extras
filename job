local function vu69(p63)
    local v64, v65, v66 = string.gmatch(p63, "([^,]+)")
    while true do
        v66 = v64(v65, v66)
        if v66 == nil then
            break
        end
        local v67, v68 = string.match(v66, "([^:]+):([^:]+)")
        if v67 and v68 then
            _G.CHATBOTHUB_CUSTOMAIS[v67] = v68
        end
    end
end
function encodeAIs()
    local v70, v71, v72 = pairs(_G.CHATBOTHUB_CUSTOMAIS)
    local v73 = {}
    while true do
        local v74
        v72, v74 = v70(v71, v72)
        if v72 == nil then
            break
        end
        table.insert(v73, v72 .. ":" .. v74)
    end
    return table.concat(v73, ",")
end
local function v77()
    if writeFileAvailable() then
