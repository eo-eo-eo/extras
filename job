local function vu52()
    if vu4.Character and vu4.Character:FindFirstChild("HumanoidRootPart") then
        local v51 = vu4.Character.HumanoidRootPart
        while _G.CHATBOTHUB_TTA_RUNNING do
            v51.CFrame = v51.CFrame * CFrame.Angles(0, math.rad(15), 0)
            wait(0.1)
        end
    end
end
local function vu55(p53)
    local v54 = vu48(p53)
    print("following " .. v54.DisplayName)
    _G.CHATBOTHUB_TARGET_PLAYER = v54
    while _G.CHATBOTHUB_TTA_RUNNING and v54 == _G.CHATBOTHUB_TARGET_PLAYER do
        vu4.Character:FindFirstChildOfClass("Humanoid"):MoveTo(v54.Character.HumanoidRootPart.Position)
        wait(0.05)
    end
end
local function vu59(p56)
    local v57 = string.lower(p56)
    if v57 == "stop" then
        vu49()
        return
    elseif v57 == "jump" then
        vu50()
        return
    elseif v57 == "spin" then
        vu52()
        return
    elseif v57 ~= "null" then
        local v58 = string.match(v57, "^follow%s+(.+)")
        if v58 then
            vu55(v58)
        end
    end
end
